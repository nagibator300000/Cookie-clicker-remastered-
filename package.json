{
  "name": "reactjson",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "buildServer": "tsc -b backend",
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "doom": "nodemon -r dotenv/config ./backend/build/backend/server.js ",
    "start": "concurrently \"npm run doom\" \"npm run dev\""
  },
  "dependencies": {
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/modifiers": "^7.0.0",
    "@prisma/client": "^5.12.1",
    "@quixo3/prisma-session-store": "^3.1.13",
    "@tanstack/react-query": "^5.51.18",
    "@types/express": "^4.17.21",
    "body-parser": "^1.20.2",
    "clsx": "^2.1.0",
    "concurrently": "^9.0.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-session": "^1.18.0",
    "nodemon": "^3.1.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "prisma": "^5.16.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-google-button": "^0.8.0",
    "react-tooltip": "^5.28.0",
    "zod": "^3.23.8",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@4tw/cypress-drag-drop": "^2.2.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express-session": "^1.18.0",
    "@types/passport-google-oauth20": "^2.0.14",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@vitejs/plugin-react": "^4.2.1",
    "cypress": "^13.15.2",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "sass": "^1.77.0",
    "typescript": "^5.2.2",
    "vite": "^5.1.6",
    "vite-plugin-checker": "^0.8.0"
  },
  "nodemonConfig": {
    "watch": [
      "./backend"
    ],
    "ext": "ts,js",
    "events": {
      "exit": "npm run buildServer"
    }
  }
}
